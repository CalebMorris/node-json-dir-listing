{
  "name": "json-dir-listing",
  "version": "0.0.3",
  "description": "Tool to generate JSON directory listings",
  "main": "index.js",
  "bin": {
    "json-dir-listing": "./bin/index.js"
  },
  "scripts": {
    "clean": "rm -Rf ./coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "./node_modules/.bin/mocha --recursive",
    "lint": "./node_modules/.bin/eslint --ext .js .",
    "lint-quiet": "./node_modules/.bin/eslint --ext .js --quiet .",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter dot --recursive"
  },
  "keywords": [
    "cli",
    "json",
    "directory",
    "fs",
    "listing",
    "workaround",
    "access"
  ],
  "author": "Caleb Morris <caleb.morris.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "temp": "^0.8.3"
  },
  "dependencies": {
    "bluebird": "^3.2.2",
    "commander": "^2.9.0"
  }
}
